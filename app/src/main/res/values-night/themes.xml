<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green_200</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorPrimaryContainer">@color/green_300</item>
        <item name="colorOnPrimaryContainer">@color/green_600</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_300</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorSecondaryContainer">@color/green_400</item>
        <item name="colorOnSecondaryContainer">@color/green_600</item>
        <item name="actionModeBackground">@android:color/transparent</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
       <item name="colorTertiary">@color/green_200</item>
       <item name="colorOnTertiary">@color/green_500</item>
       <item name="colorTertiaryContainer">@color/green_700</item>
       <item name="colorOnTertiaryContainer">@color/green_700</item>
    </style>
</resources>